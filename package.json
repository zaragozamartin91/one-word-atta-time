{
  "name": "one-word-atta-time",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "build": "browserify ./src/front.js -o ./public/javascripts/front-bundle.js",
    "watch": "watchify ./src/front.js -o ./public/javascripts/front-bundle.js",
    "watch-react": "npx babel --watch public/javascripts/components --out-dir ./public/javascripts/bundled --presets react-app/prod"
  },
  "dependencies": {
    "arraybuffer-to-buffer": "^0.0.7",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-react-app": "^3.1.2",
    "browserify": "^17.0.0",
    "watchify": "^4.0.0",
    "webpack": "^5.61.0",
    "webpack-cli": "^4.9.1"
  }
}
